{
  "root": "dist/",
  "clean_urls": false,
  "routes": {
    "/**": "index.html"
  },
  "proxies": {
    "/api/": {
      "origin": "https://your-backend-url.railway.app"
    },
    "/ws/": {
      "origin": "https://your-backend-url.railway.app"
    }
  },
  "headers": {
    "/assets/**": {
      "Cache-Control": "public, max-age=31536000"
    }
  }
}

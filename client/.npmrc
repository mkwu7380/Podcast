# Save exact version of packages in package-lock.json
save-exact = true

# Use package-lock.json (enabled by default in npm 5+)
package-lock = true

# Disable package-lock.json creation
# package-lock = false

# Set the package manager to use
engine-strict = true

# Set the registry (if using a private registry)
# registry=https://registry.npmjs.org/

# Set the cache directory
cache = .npm-cache

# Set the log level (silent, error, warn, notice, http, timing, info, verbose, silly)
loglevel = warn

# Disable progress bars for CI environments
progress = true

# Disable color output for CI environments
color = true

# Set the user agent
user-agent = npm/{npm-version} node/{node-version} {platform} {arch} {ci}

# Set the tmp directory
tmp = /tmp

# Set the prefix for global installations
prefix = ~/.npm-global

# Set the save-prefix (^ by default)
save-prefix = ~

# Set the script-shell (default is /bin/sh)
# script-shell = /bin/bash

# Disable scripts for security
# ignore-scripts = true

# Set the package-lock lockfile version
lockfile-version = 2

# Set the package-lock only (don't touch node_modules)
package-lock-only = false

# Set the package-lock update strategy
# prefer-dedupe = true

# Set the package-lock update strategy
# prefer-offline = true

